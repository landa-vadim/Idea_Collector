<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:src="@drawable/rectangle_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_2"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/redCoub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="27dp"
        android:src="@drawable/rectangle_3"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <ImageView
        android:layout_width="11dp"
        android:layout_height="8dp"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="-2dp"
        android:background="@android:color/transparent"
        android:src="@drawable/ic_polygon"
        app:layout_constraintBottom_toBottomOf="@id/redCoub"
        app:layout_constraintEnd_toEndOf="@id/redCoub"/>

    <ImageButton
        android:layout_width="18dp"
        android:layout_height="26dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/transparent"
        android:src="@drawable/ic_idea"
        app:layout_constraintStart_toStartOf="@+id/redCoub"
        app:layout_constraintTop_toTopOf="@+id/redCoub" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="13dp"
        android:background="@android:color/transparent"
        android:src="@drawable/ic_done"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="21dp"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="196dp"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintStart_toStartOf="@id/view2"
        android:src="@drawable/line_1" />

    <ImageView
        android:layout_width="21dp"
        android:layout_height="2dp"
        android:layout_marginTop="14dp"
        android:layout_marginStart="196dp"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintStart_toStartOf="@id/view2"
        android:src="@drawable/line_2" />

    <ImageView
        android:layout_width="21dp"
        android:layout_height="2dp"
        android:layout_marginTop="18dp"
        android:layout_marginStart="196dp"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintStart_toStartOf="@id/view2"
        android:src="@drawable/line_3" />

    <ImageView
        android:id="@+id/editText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:src="@drawable/et_vector_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/redCoub"
        android:layout_marginStart="26dp"
        android:layout_marginTop="58dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line_5"
        app:layout_constraintBottom_toBottomOf="@id/editText"
        app:layout_constraintStart_toStartOf="@id/editText"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="3dp" />

    <View
        android:id="@+id/view4"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/bg1"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:src="@drawable/m_bg_1"
        android:adjustViewBounds="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:src="@drawable/m_bg_1"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="304dp"
        android:adjustViewBounds="true"
        android:src="@drawable/m_bg_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:src="@drawable/vector"
        android:layout_marginStart="112dp"
        android:layout_marginTop="300dp"
        app:layout_constraintTop_toTopOf="@id/view4"
        app:layout_constraintStart_toStartOf="@id/view4" />

</androidx.constraintlayout.widget.ConstraintLayout>